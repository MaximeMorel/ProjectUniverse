////////////////////////////////////////////////////////////////////////////////
#include "vec2.hpp"
////////////////////////////////////////////////////////////////////////////////
template <typename T>
TVec2<T>::TVec2()
: x(0), y(0)
{
}
////////////////////////////////////////////////////////////////////////////////
template <typename T>
TVec2<T>::TVec2(const T& x, const T& y)
: x(x), y(y)
{
}
////////////////////////////////////////////////////////////////////////////////
template <typename T>
size_t TVec2<T>::getMemSize() const
{
    return sizeof(*this);
}
